{
  "name": "nhan-starter",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "dotenv -e .env.local -- vite dev --port 3000",
    "inngest:dev": "npx inngest-cli@latest dev -u http://localhost:3000/api/inngest",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest run",
    "lint": "eslint",
    "format": "prettier",
    "check": "prettier --write . && eslint --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "setup-db-local": "docker compose --env-file .env.local up",
    "db:generate": "dotenv -e .env.local -- drizzle-kit generate",
    "db:migrate": "dotenv -e .env.local -- drizzle-kit migrate",
    "db:push": "dotenv -e .env.local -- drizzle-kit push",
    "db:studio": "dotenv -e .env.local -- drizzle-kit studio",
    "db:seed": "dotenv -e .env.local -- tsx src/db/seed.ts"
  },
